# Databricks NEMWEB Lab - Environment Variables
# Copy this file to .env and fill in your values

# Databricks workspace connection
DATABRICKS_HOST=https://your-workspace.cloud.databricks.com
DATABRICKS_TOKEN=dapi_your_token_here

# Unity Catalog configuration
NEMWEB_CATALOG=main
NEMWEB_SCHEMA=nemweb_lab

# SQL Warehouse (for Databricks App queries)
DATABRICKS_WAREHOUSE_ID=your_warehouse_id

# App data source configuration
NEMWEB_DATA_SOURCE=delta
NEMWEB_DELTA_TABLE=main.nemweb_lab.nemweb_raw
